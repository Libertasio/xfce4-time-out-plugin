#. Create menu item for taking an instant break
#: ../panel-plugin/time-out.c:264
msgid "Take a break"
msgstr "Tartson szünetet"

#. Create menu item for resetting the timer
#: ../panel-plugin/time-out.c:272
msgid "Reset timer"
msgstr "Időzítő visszaállítása"

#. Create menu item for enabling/disabling the countdown
#: ../panel-plugin/time-out.c:280
msgid "Enabled"
msgstr "Engedélyezve"

#: ../panel-plugin/time-out.c:421
msgid ""
"Xfce Panel plugin for taking a break from computer work every now and then."
msgstr "Xfce panelbővítmény rendszeres szünetek tartásához."

#: ../panel-plugin/time-out.c:430
msgid "translator-credits"
msgstr "Kelemen Gábor <kelemeng at gnome dot hu>"

#. Create properties dialog
#: ../panel-plugin/time-out.c:492
#: ../panel-plugin/xfce4-time-out-plugin.desktop.in.h:1
msgid "Time Out"
msgstr "Időkorlát"

#. Create time settings section
#: ../panel-plugin/time-out.c:509
msgid "Time settings"
msgstr "Időbeállítások"

#. Create the labels for the minutes and seconds spins
#: ../panel-plugin/time-out.c:522
msgid "Minutes"
msgstr "Perc"

#: ../panel-plugin/time-out.c:527
msgid "Seconds"
msgstr "Másodperc"

#. Create break countdown time label
#: ../panel-plugin/time-out.c:533
msgid "Time between breaks:"
msgstr "Szünetek közti idő:"

#. Create lock countdown time label
#: ../panel-plugin/time-out.c:557
msgid "Break length:"
msgstr "Szünet hossza:"

#. Create postpone countdown time label
#: ../panel-plugin/time-out.c:575
msgid "Postpone length:"
msgstr "Elhalasztás hossza:"

#. Create behaviour section
#: ../panel-plugin/time-out.c:593
msgid "Behaviour"
msgstr "Viselkedés"

#. Create postpone check button
#: ../panel-plugin/time-out.c:604
msgid "Allow postpone"
msgstr "Elhalasztás engedélyezése"

#. Create resume check button
#: ../panel-plugin/time-out.c:611
msgid "Resume automatically"
msgstr "Automatikus folytatás"

#. Create appearance section
#: ../panel-plugin/time-out.c:618
msgid "Appearance"
msgstr "Megjelenés"

#. Create display time check button
#: ../panel-plugin/time-out.c:629
msgid "Display remaining time in the panel"
msgstr "Hátralévő idő megjelenítése a panelen"

#. Create display hours check button
#: ../panel-plugin/time-out.c:636
msgid "Display hours"
msgstr "Órák megjelenítése"

#. Create display seconds check button
#: ../panel-plugin/time-out.c:643
msgid "Display seconds"
msgstr "Másodpercek megjelenítése"

#. Create postpone button
#: ../panel-plugin/time-out-lock-screen.c:219
msgid "_Postpone"
msgstr "Elhalasz_tás"

#. Create resume button
#: ../panel-plugin/time-out-lock-screen.c:225
msgid "_Resume"
msgstr "_Folytatás"

#: ../panel-plugin/time-out-countdown.c:377
msgid "The break is over."
msgstr "Vége a szünetnek"

#. Hours:minutes:seconds
#: ../panel-plugin/time-out-countdown.c:399
#, c-format
msgid "%02d:%02d:%02d"
msgstr "%02d.%02d.%02d"

#. Hours:minutes
#. Minutes:seconds
