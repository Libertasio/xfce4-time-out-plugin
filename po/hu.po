#: ../panel-plugin/time-out-countdown.c:412
#, c-format
msgid "%02d:%02d"
msgstr "%02d.%02d"

#: ../panel-plugin/time-out-countdown.c:424
#, c-format
msgid "%d hour"
msgid_plural "%d hours"
